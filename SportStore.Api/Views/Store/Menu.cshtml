@model IEnumerable<SportStore.Api.ViewModels.CategoryViewModel>

<section class="container" style="max-height: 10%">
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <a href="@Url.Action("List", "Store", new { currentCategory = item.Name})">
                    <img class="icon" src="~/Content/Icons/@item.IconFileName" />
                    <br />
                    @item.Name
                </a>
            </li>
        }
    </ul>

</section>